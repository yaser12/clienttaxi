<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="clienttaxi.clienttaxi.CallingTaxiActivity"
    tools:showIn="@layout/activity_calling_taxi">

    <TextView
        android:id="@+id/editText3"
        android:layout_width="398dp"
        android:layout_height="14dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="تم ارسال طلب للتكسي من قبل مكتب التكسي"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="5dp" />

    <TextView
        android:id="@+id/orderoutput"
        android:layout_width="match_parent"
        android:layout_height="678dp"
        android:layout_marginTop="8dp"
        android:text="الوقت المتبقي للوصول هو ربع ساعة ...."
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="33dp" />
</android.support.constraint.ConstraintLayout>
